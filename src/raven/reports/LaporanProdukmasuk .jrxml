	<queryString>
		<![CDATA[SELECT 
  pm.id as no,
  b.kode_barang,
  b.nama_barang,
  pm.jumlah_masuk,
  pm.tanggal_masuk,
  pm.tanggal_kadaluarsa
FROM 
  tabel_produkmasuk pm
JOIN 
  tabel_barang b ON pm.kode_barang = b.kode_barang
ORDER BY
  pm.id ASC;]]>
	</queryString>
